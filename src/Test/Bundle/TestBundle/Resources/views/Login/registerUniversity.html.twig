{% extends baseLayout %}

{% block title %}TestBundle:Login:Register website Form{% endblock %}

{% block form %}
{{ form_start(form, {attr:{novalidate: 'novalidate'}}) }}
<table>
   
     <tr>
        <td align="left">{{form_label(form.name)}}</td>
        <td>{{form_widget(form.name)}}</td>
        <td>{{form_errors(form.name)}}</td>
    </tr>
    <tr>
        <td align="left">{{form_label(form.subdomain)}}</td>
        <td>{{form_widget(form.subdomain)}}</td>
        <td>{{form_errors(form.subdomain)}}</td>
    </tr>
</table>
{{form_end(form)}}
{% endblock %}