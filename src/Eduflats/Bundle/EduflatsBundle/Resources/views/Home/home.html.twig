        <span class="sr-only">Toggle navigation</span>
        <span><i class="fa fa-search"></i></span>
    </button>
</div>

<div class="ct-topBar ">
    <div class="container">
        <div class="row">
            <div class="col-sm-7 col-md-8">
                <div class="ct-panel--contact ct-panel--left">
                    <ul class="nav navbar-nav ct-switcher--language">
                        <li class="dropdown">
                            <a href="#">
                                <i class="fa fa-globe"></i>
                                English
                                <i class="fa fa-angle-down"></i>
                            </a>
                            <ul class="dropdown-menu">
                                <li>
                                    <a href="#">
                                        English
                                    </a>
                                </li>
                                <li>
                                    <a href="#">
                                        French
                                    </a>
                                </li>
                            </ul>
                        </li>
                    </ul>
                    <div id="ct-js-navSearch">
                        <i class="fa fa-search"></i>
                        <input placeholder="Search ..." required type="text" name="field[]" class="form-control input-lg ct-input--search">
                    </div>
                    <div class="ct-navbar-search">
                        <form role="form">
                            <button class="ct-navbar-search-button" type="submit">
                                <i class="fa fa-search fa-fw"></i>
                            </button>
                            <div class="form-group">
                                <input id="search" placeholder="Search ..." required type="text" class="form-control input-lg">
                            </div>
                        </form>
                    </div>
                </div>
            </div>
            <div class="col-sm-5 col-md-4">
                <div class="ct-panel--user ct-panel--right text-right">
                    {% if not app.user %}
                        <div class="ct-panel--item ct-email">
                            <a href="{{ path('login') }}"><i class="fa fa-user"></i> Login</a>
                        </div>
                    {% endif %}
                    {% if app.user %}
                    <!-- if authenticated fully -->
	                    <div class="ct-panel--item ct-email">
	                        <a href=""><i class="fa fa-home"></i> Wishlist</a>
	                    </div>
	            <!-- end -->
                    {% endif %}
                    {% if app.user %}
                    <!-- if authenticated fully -->
                    <div class="ct-panel--item ct-email">
                        <ul class="nav navbar-nav ct-switcher--language ct-switcher--withDecoration">
                            <li class="dropdown" style="cursor: pointer; cursor: hand;">
                                <div class="ct-userImage" ><img src="{{ asset('bundles/eduflats/images/demo-content/user-small.jpg') }}" alt="user"></div> My Account
                                <ul class="dropdown-menu">
                                    <li>
                                        <a href="#">
                                            <i class="fa fa-newspaper-o"></i>Agency Settings
                                        </a>
                                    </li>
                                    <li>
                                        <a href="#">
                                            <i class="fa fa-briefcase"></i>Agent Settings
                                        </a>
                                    </li>
                                    <li>
                                        <a href="#">
                                            <i class="fa fa-list"></i>My Properties
                                        </a>
                                    </li>
                                    <li>
                                        <a href="#">
                                            <i class="fa fa-cog"></i>Account Settings
                                        </a>
                                    </li>
                                    <li>
                                        <a href="#">
                                            <i class="fa fa-power-off"></i>Sign Out
                                        </a>
                                    </li>
                                </ul>
                            </li>
                        </ul>
                    </div>
                    <!-- end -->
                    {% endif %}
                </div>
            </div>
        </div>
    </div>
</div>
    
